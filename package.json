{
  "name": "shroom-io",
  "version": "1.0.0",
  "description": "Spread your mushroom kingdom and defeat other colonies",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "tsc && node --enable-source-maps ./server-dist/server-src/index.js",
    "build": "npm run build-client",
    "lint": "eslint client-src --ext .js,.ts,.tsx",
    "webpack": "webpack",
    "watch-webpack": "webpack-dev-server --progress --color --mode development --config webpack.config.js",
    "build-client": "webpack --mode production",
    "watch-node": "nodemon server-dist/server.js",
    "dev": "npm run watch-webpack",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicksonzero/shroom-io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vicksonzero/shroom-io/issues"
  },
  "homepage": "https://github.com/vicksonzero/shroom-io#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "socket.io": "^4.5.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "@flyover/box2d": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "debug": "^4.3.4",
    "eslint": "^8.19.0",
    "expose-loader": "^4.0.0",
    "js-yaml": "^4.1.0",
    "json-loader": "^0.5.7",
    "phaser": "^3.55.2",
    "raw-loader": "^4.0.2",
    "socket.io-client": "^4.5.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack-dev-server": "^4.9.3",
    "yaml-loader": "^0.8.0"
  }
}
